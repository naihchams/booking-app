{
  "name": "booking-app",
  "description": "A booking app built with React and Electron",
  "author": "Nai Hamdan",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "bin": "App.js",
  "proxy": "https://smartoffice.dubaifuture.ae",
  "dependencies": {
    "@azure/msal-browser": "^4.5.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.7",
    "@placeos/ts-client": "^3.25.22",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "luxon": "^3.5.0",
    "react": "^18.2.0",
    "react-datepicker": "^8.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.0",
    "react-scripts": "5.0.1",
    "rxjs": "^7.8.2",
    "ts-md5": "^1.3.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.yourname.bookingapp",
    "productName": "Booking App",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "files": [
      "**/*",
      "build/**/*",
      "public/electron.js"
    ],
    "dmg": {
      "background": "build/dmg-background.png",
      "icon": "build/icon.icns"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@nrwl/node": "^19.8.4",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2"
  }
}
